{
  "name": "yproximite/yprox-api-client",
  "type": "library",
  "description": "Client library for yProx API",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikita Sviridenko",
      "email": "mail.sviridenko@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Yproximite/Symfony2-coding-standard"
    }
  ],
  "require": {
    "php": "^7.0",
    "php-http/message-factory": "^1.0.2",
    "php-http/httplug": "^1.0",
    "php-http/discovery": "^1.0",
    "symfony/property-access": "^2.8 || ^3.0",
    "php-http/multipart-stream-builder": "^1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.2",
    "php-http/message": "^1.0",
    "php-http/guzzle6-adapter": "^1.0",
    "m6web/Symfony2-coding-standard": "dev-master",
    "m6web/coke": "^2.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Api\\": "src"
    }
  }
}
