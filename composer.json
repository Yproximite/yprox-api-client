{
  "name": "yproximite/yprox-api-client",
  "type": "library",
  "description": "Client library for yProx API",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikita Sviridenko",
      "email": "mail.sviridenko@gmail.com"
    }
  ],
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix -v",
    "php-cs-fixer@ci": "php-cs-fixer fix -v --diff --dry-run",
    "phpspec": "phpspec --ansi"
  },
  "require": {
    "php": "^7.3",
    "php-http/message-factory": "^1.0.2",
    "php-http/httplug": "^1.0",
    "php-http/discovery": "^1.0",
    "symfony/property-access": "^4.0 || ^5.0",
    "php-http/multipart-stream-builder": "^0.1 || ^0.2 || ^1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^6.0",
    "php-http/message": "^1.0",
    "php-http/guzzle6-adapter": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Api\\": "src"
    }
  }
}
